<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Procesar</servlet-name>
        <servlet-class>misrc.Procesar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProcesarGet</servlet-name>
        <servlet-class>misrc.ProcesarGet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProcesarPost</servlet-name>
        <servlet-class>misrc.ProcesarPost</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Suma</servlet-name>
        <servlet-class>misrc.Suma</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletLogin</servlet-name>
        <servlet-class>dispatcher.ServletLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletBienvenida</servlet-name>
        <servlet-class>dispatcher.ServletBienvenida</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Controlador</servlet-name>
        <servlet-class>dispatcher.Controlador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Consultas</servlet-name>
        <servlet-class>dispatcher.Consultas</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Procesar</servlet-name>
        <url-pattern>/Procesar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProcesarGet</servlet-name>
        <url-pattern>/ProcesarGet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProcesarPost</servlet-name>
        <url-pattern>/ProcesarPost</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Suma</servlet-name>
        <url-pattern>/Suma</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletLogin</servlet-name>
        <url-pattern>/ServletLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletBienvenida</servlet-name>
        <url-pattern>/ServletB</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Controlador</servlet-name>
        <url-pattern>/Controlador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Consultas</servlet-name>
        <url-pattern>/Consultas</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
